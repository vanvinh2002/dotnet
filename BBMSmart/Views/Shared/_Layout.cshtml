<script type="text/javascript">

    $(function () {
        $.ajax({
            type: "POST",
            url: "/Account/getNoti",
            data: JSON.stringify({}),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {
                $('#notiarea').empty();
                var str = '';
                $.each(r, (index, value) => {
                 
                    var tr = '<li>•&nbsp <a href="/' + value.controller + '/' + value.action + '">' + value.fname + '</a> <span style="float:right;font-weight: normal;font-size:smaller" class="redtext">' + (value.waiting > 0 ? '('+value.waiting +')' : '')+'</span></li> ';
                    str += tr;
                });
                $("#notiarea").append(str);
            }
        });
    });



    $("#icon_bar").hover(
        function () {
            $("#menu").show();
        }, function () {
        });

    $("#menu").hover(
        function () {
        }, function () {
            $("#menu").hide();
        });

    $("#menuNoti").hover(
