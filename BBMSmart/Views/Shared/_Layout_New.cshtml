<script>

    $(function () {
        $('#Heloword').html(new Date().getHours() >= 12 ? "Chào buổi chiều!" : "Chào buổi sáng!");

        $.ajax({
            type: "POST",
            url: "/Account/getNoti",
            data: JSON.stringify({}),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {
                $('#notiarea').empty();
                var str = '';
                $.each(r, (index, value) => {
                    if (value.waiting > 0) $('#notifysignal2').show();
                    var tr = '<li> <a class="notiline" href="/' + value.controller + '/' + value.action + '">' + value.fname + '</a> <span style="float:right;font-weight: normal;font-size:smaller" class="redtext">' + (value.waiting > 0 ? '(' + value.waiting + ')' : '') + '</span></li> ';
                    str += tr;
                });
                $("#notiarea").append(str);
                
            }
        });

        $.ajax({
            type: "POST",
            url: "/Notification/GetNotiNV",
            data: JSON.stringify({}),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {
                if (r.length > 0) {
                    $('#notiarea2').empty();
                    lstnoti = r;
                var str = '';
                $.each(r, (index, value) => {

                    var tr = '<li> <a class="notiline" onclick="js_detailmodal(' + index + ')" >'
                        + value.Title + '</a> </li> ';
                    str += tr;
                });
                    $("#notiarea2").append(str);
                    $('#notifysignal').show();
                    js_detailmodal('0');
                }
               
            }
        });

    });

    var currentID = 0;
    var lstnoti;
    function js_detailmodal(index) {
        currentID = lstnoti[index].ID;
        $('#detailmodal').html(lstnoti[index].Detail);
        $('#Detail_modal').modal('show');
    }

    function js_close() {
        $('#Detail_modal').modal('hide');
    }

    function js_read() {
        var NotiID = currentID;
        $.ajax({
            url: '/Notification/ReadNoti',
            type: 'POST',
            data: JSON.stringify({ NotiID: NotiID }),
            contentType: 'application/json, charset=utf-8',
            beforeSend: function () {
               
            },
            success: function (data) {
                if (data.result == "1") {
                  
                    location.reload(true);
                } else {
                  
                    jAlert("Có lỗi xảy ra." + data.result);
                }
            },
            error: function (err) {
              

            }
        });
    }
</script>
